{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.2",
    "antd": "^4.2.0",
    "axios": "^0.19.2",
    "braft-editor": "^2.3.9",
    "gg-editor": "^3.1.1",
    "lint-staged": "^10.0.7",
    "markdown-it": "^10.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-datasheet": "^1.4.3",
    "react-dom": "^16.12.0",
    "react-infinite-scroller": "^1.2.4",
    "react-markdown-editor-lite": "^1.1.2",
    "umi": "^3.1.2",
    "yorkie": "^2.0.0"
  }
}
